{
  "name": "linkedup-backend",
  "version": "0.1.0",
  "bin": {
    "linkedup-backend": "bin/linkedup-backend.js"
  },
  "scripts": {
    "clean": "rimraf ./node_modules src/__generated__ dist",
    "openapi:generate": "openapi-typescript ./resource/openapi/api-spec.yaml -o src/__generated__/openapi/schema.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "ts-node src/db/seed/seed.ts",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest",
    "lint": "eslint .",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "aws-cdk": "2.1006.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@2toad/profanity": "^3.1.1",
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/s3-request-presigner": "^3.823.0",
    "@matsukky/perspective-client": "^0.1.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.203.0",
    "@opentelemetry/instrumentation-express": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/propagator-b3": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@types/opencc-js": "^1.0.3",
    "@vendia/serverless-express": "^4.12.6",
    "aws-cdk-lib": "2.185.0",
    "body-parser": "^2.2.0",
    "constructs": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "diff": "^8.0.2",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "jwt-decode": "^4.0.0",
    "openapi-typescript": "^7.6.1",
    "opencc-js": "^1.0.5",
    "pg": "^8.16.3",
